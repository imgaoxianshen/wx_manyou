page{
   background-color:#fff;
}
.start_container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.start-circle{
  margin-top:100rpx;
  width:400rpx;
  height:400rpx;
  background-color:rgb(230,230,70);
  border-radius: 50%;
  box-shadow: 5px 3px 10px #888888;
}

.start-cloud{
  width:200rpx;
  height:200rpx;
  position:relative;
  top:-200rpx;
  left:-100rpx;
}

.start-text{
  margin-top:30rpx;
}