.center-list-icon {
  width: 50rpx;
  height: 50rpx;
  margin-left:20rpx;
}

.center-top {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 300rpx;
  background-color:#FFE328;
}

.center-headimg {
  width: 100rpx;
  height: 100rpx;
  border: 1px solid #fff;
  border-radius: 50%;
  background-color:#fff;
}

.center-text {
  margin-top: 30rpx;
  color:#fff;
}

.center-list{
  width:90%;
  margin: 0 auto;
  border-radius:30rpx;
  background-color:white;
  box-shadow: -2px 2px 2px #d1d1d1;
  margin-top:40rpx;
}

.list-item {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.list-text{
  margin-left:20rpx;
}

